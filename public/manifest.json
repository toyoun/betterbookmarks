{
  "manifest_version": 2,

  "name": "Test",
  "version": "0.0.1",
  "description": "Display and navigate your bookmarks on the new tab page!",

  "browser_action": {
    "default_title": "New Tab"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "permissions": [
    "bookmarks"
  ],

  "content_security_policy": "script-src 'self' https://www.google.com https://api.bing.com; object-src 'self'"

}