{
  "manifest_version": 2,

  "name": "Better Bookmarks",
  "version": "0.0.1",
  "description": "Display and navigate your bookmarks on the new tab page!",

  "icons": {
    "32": "better-bookmarks-favicon32.png",
    "64": "better-bookmarks-favicon64.png",
    "128": "better-bookmarks-favicon128.png"
  },

  "browser_action": {
    "default_title": "Better Bookmarks"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "permissions": [
    "bookmarks",
    "storage"
  ],

  "content_security_policy": "script-src 'self'; object-src 'self'"

}